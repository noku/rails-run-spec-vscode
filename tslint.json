{
  "extends": ["tslint-microsoft-contrib", "tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "chai-vague-errors": false,
    "export-name": false,
    "function-name": false,
    "insecure-random": false,
    "jsx-no-lambda": false,
    "jsx-wrap-multiline": false,
    "jsx-no-multiline-js": false,
    "max-func-body-length": false,
    "missing-jsdoc": false,
    "mocha-no-side-effect-code": false,
    "no-any": false,
    "no-backbone-get-set-outside-model": false,
    "no-http-string": false,
    "no-implicit-dependencies": false,
    "no-increment-decrement": false,
    "no-namespace": false,
    "no-non-null-assertion": false,
    "no-object-literal-type-assertion": false,
    "no-reserved-keywords": false,
    "no-single-line-block-comment": false,
    "no-submodule-imports": false,
    "no-suspicious-comment": false,
    "no-null-keyword": false,
    "no-this-assignment": false,
    "no-typeof-undefined": false,
    "no-unused-expression": false,
    "no-useless-files": false,
    "object-literal-sort-keys": false,
    "prefer-array-literal": false,
    "prefer-object-spread": false,
    "prefer-type-cast": false,
    "quotemark": false,
    "radix": false,
    "react-a11y-anchors": false,
    "react-a11y-event-has-role": false,
    "react-a11y-image-button-has-alt": false,
    "react-a11y-img-has-alt": false,
    "react-a11y-role-has-required-aria-props": false,
    "react-a11y-role": false,
    "react-this-binding-issue": false,
    "react-a11y-input-elements": false,
    "react-a11y-required": false,
    "switch-default": false,
    "switch-final-break": false,
    "prefer-while": false,
    "trailing-comma": false,
    "typedef": [true, "call-signature", "parameter"],
    "variable-name": [true, "allow-leading-underscore", "allow-pascal-case"],
    "no-unnecessary-local-variable": false,
    "use-default-type-parameter": false,
    "completed-docs": false,
    "no-void-expression": false,
    "no-floating-promises": false,
    "no-unsafe-any": false,
    "promise-function-async": false,
    "no-cookies": false,
    "strict-boolean-expressions": false,
    "callable-types": false,

    "file-name": true,
    "todo": true
  },
  "linterOptions": {
    "exclude": ["**/*.d.ts"]
  },
  "rulesDirectory": ["./lintingRules"]
}
